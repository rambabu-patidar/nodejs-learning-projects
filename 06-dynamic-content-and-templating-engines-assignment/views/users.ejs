<%- include("./includes/head.ejs") %>
  <link rel="stylesheet" href="/css/users.css">
  </head>

  <body>
    <%- include("./includes/navigation.ejs") %>

      <main class="main">
        <% if (users.length> 0) { %>
          <ul class="user-list">
            <% for (let user of users) { %>
              <li class="user-list-item">
                <%= user.name %>
              </li>
              <% }%>
          </ul>
          <% } else {%>
            <h1>No Users Found!!</h1>
            <%} %>
      </main>

      <%- include("./includes/end.ejs") %>